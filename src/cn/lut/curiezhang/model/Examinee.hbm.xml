<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
  <class name="cn.lut.curiezhang.model.Examinee" table="EXAMINEE">
    <id name="examineeId">
			<column name="EXAMINEE_ID" length="32">
				<comment>用户id</comment>
			</column>
    	<generator class="assigned"/>
    </id>
    <property name="examineeName">
			<column name="EXAMINEE_NAME" not-null="true" length="50">
				<comment>用户名</comment>
			</column>
    </property>
    <property name="examineePassword">
			<column name="EXAMINEE_PASSWORD" not-null="true" length="128">
				<comment>用户密码</comment>
			</column>
    </property>
    <property name="examineePhone">
			<column name="EXAMINEE_PHONE" length="20">
				<comment>用户手机</comment>
			</column>
    </property>
    <property name="examineeCreator">
      <column name="EXAMINEE_CREATOR" length="32">
        <comment>创建人</comment>
      </column>
    </property>
    <property name="examineeCreatingTime" type="java.util.Date">
      <column name="EXAMINEE_CREATING_TIME">
        <comment>创建时间</comment>
      </column>
    </property>
    <property name="examineeAuditor">
      <column name="EXAMINEE_AUDITOR" length="32">
        <comment>审核人</comment>
      </column>
    </property>
    <property name="examineeAuditingTime" type="java.util.Date">
      <column name="EXAMINEE_AUDITING_TIME">
        <comment>审核时间</comment>
      </column>
    </property>
    <!-- 0学生 1院校 2考点 3管理员 10锁定学生 11锁定院校 12锁定考点 13锁定管理员 90~93找回密码 -->
    <property name="status">
      <column name="STATUS" default="0" length="2">
        <comment>用户状态：0学生 1院校 2考点 3管理员 4锁定学生 5锁定院校 6锁定考点 7锁定管理员 8找回学生密码 9找回院校密码 10找回考点密码 11找回管理员密码</comment>
      </column>
      <type name="org.hibernate.type.EnumType">
        <param name="enumClass">cn.lut.curiezhang.model.ExamineeStatus</param>
        <param name="type">-6</param>
        <!-- <param name="useNamed">true</param> -->
      </type>
    </property>
  </class>
</hibernate-mapping>